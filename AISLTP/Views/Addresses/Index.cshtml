@model IEnumerable<AISLTP.Entities.Address>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Np.Txt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Obl.Txt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rn.Txt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ul)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Korpus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kvartira)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Np.Txt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Obl.Txt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rn.Txt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ul)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Korpus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kvartira)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
